	<queryString language="SQL">
		<![CDATA[SELECT
     CLIENTES."CED_CLI" AS CLIENTES_CED_CLI,
     CLIENTES."NOM_CLI" AS CLIENTES_NOM_CLI,
     CLIENTES."APE_CLI" AS CLIENTES_APE_CLI,
     CLIENTES."TEL_CLI" AS CLIENTES_TEL_CLI,
     VENTAS."NUM_VEN" AS VENTAS_NUM_VEN,
     VENTAS."FEC_VEN" AS VENTAS_FEC_VEN,
     VENTAS."TOTAL" AS VENTAS_TOTAL,
     PRODUCTO_CALZADO."NOM_PRO" AS PRODUCTO_CALZADO_NOM_PRO,
     PRODUCTO_CALZADO."MAR_PRO" AS PRODUCTO_CALZADO_MAR_PRO,
     PRODUCTO_CALZADO."MOD_PRO" AS PRODUCTO_CALZADO_MOD_PRO,
     PRODUCTO_CALZADO."COD_TALL_P" AS PRODUCTO_CALZADO_COD_TALL_P,
     PRODUCTO_CALZADO."PRE_PRO" AS PRODUCTO_CALZADO_PRE_PRO,
     PRODUCTO_CALZADO."COLOR" AS PRODUCTO_CALZADO_COLOR,
     DETALLE_VENTA."CANTIDAD" AS DETALLE_VENTA_CANTIDAD,
     CLIENTES."DIR_CLI" AS CLIENTES_DIR_CLI
FROM
     "PROYECTOWD"."CLIENTES" CLIENTES INNER JOIN "PROYECTOWD"."VENTAS" VENTAS ON CLIENTES."CED_CLI" = VENTAS."CED_CLI_V"
     INNER JOIN "PROYECTOWD"."DETALLE_VENTA" DETALLE_VENTA ON VENTAS."NUM_VEN" = DETALLE_VENTA."NUM_VER"
     INNER JOIN "PROYECTOWD"."PRODUCTO_CALZADO" PRODUCTO_CALZADO ON DETALLE_VENTA."COD_PRO_V" = PRODUCTO_CALZADO."COD_PRO"]]>
	</queryString>
